{
    "name": "theodorejb/php-stv",
    "description": "Determine winning candidate(s) in an STV election",
    "type": "project",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Theodore Brown",
            "email": "theodorejb@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-dom": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "psalm/plugin-phpunit": "^0.19.5",
        "vimeo/psalm": "^6.15"
    },
    "autoload": {
        "psr-4": {
            "theodorejb\\PhpStv\\": "php/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "theodorejb\\PhpStv\\Tests\\": "test/php/"
        }
    },
    "config": {
      "platform": {
        "php": "8.2.30"
      },
      "sort-packages": true
    },
    "scripts": {
        "analyze": "psalm",
        "test": "phpunit test/php"
    }
}
